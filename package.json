        "compression": "^1.8.1",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "helmet": "^8.1.0",
        "ioredis": "^5.8.1",
        "jalali-moment": "^3.3.11",
        "joi": "^18.0.1",
        "jsonwebtoken": "^9.0.2",
        "moment": "^2.30.1",
        "mysql2": "^3.15.2",
