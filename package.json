    "main": ".",
    "bin": "dist/index.js",
    "pkg": {
        "targets": [
            "node18-linux-x64"
        ],
        "output": "dist/",
        "options": {
            "nodeArgs": [
                "--max-old-space-size=4096"
            ]
        }
